{
  "name": "siteed-ui-starter-kit",
  "version": "0.2.0",
  "description": "shared ui library",
  "main": "src/index.ts",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "prestorybook:device": "jq '.main = \"storybook_ondevice.js\"' package.json > tmp.json && mv tmp.json package.json",
    "storybook:device": "yarn sb-rn-get-stories --config-path .ondevice && expo start",
    "poststorybook:device": "jq '.main = \"src/index.ts\"' package.json > tmp.json && mv tmp.json package.json",
    "build-storybook": "storybook build",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/deeeed/ui-starter-kit",
  "author": "deeeed <abreton@siteed.net> (https://github.com/deeeed)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deeeed/ui-starter-kit/issues"
  },
  "homepage": "https://github.com/deeeed/ui-starter-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-paper": "*",
    "react-native-safe-area-context": "*",
    "react-native-vector-icons": "*"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-community/datetimepicker": "^7.4.0",
    "@react-native-community/eslint-config": "^3.0.2",
    "@react-native-community/slider": "^4.4.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/addon-onboarding": "^1.0.7",
    "@storybook/addon-ondevice-actions": "^6.5.4",
    "@storybook/addon-ondevice-controls": "^6.5.4",
    "@storybook/addon-react-native-web": "^0.0.21",
    "@storybook/blocks": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-native": "^6.5.4",
    "@storybook/react-webpack5": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^28.1.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expo": "^49.0.3",
    "expo-asset": "^8.10.1",
    "expo-cli": "^6.3.10",
    "expo-font": "^11.4.0",
    "expo-modules-core": "^1.5.7",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.3",
    "react-native-builder-bob": "^0.20.0",
    "react-native-paper": "^5.9.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "^0.19.6",
    "react-test-renderer": "^18.2.0",
    "release-it": "^15.0.0",
    "storybook": "^7.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
